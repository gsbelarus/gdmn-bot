{
  "name": "gdmn-bot",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "build": "tsc",
    "dev": "nodemon src/server.ts",
    "rebuild": "yarn clean && yarn build",
    "download-data": "node src/util/downloadData.js",
    "start": "yarn rebuild && ts-node ./src/server.ts",
    "start:old": "tsc && node ./dist/echo-bot.js",
    "start:dev": "cross-env NODE_ENV=dev && yarn start",
    "watch": "yarn rebuild && nodemon -e ts -w ./src -x yarn watch:serve",
    "watch:serve": "node --harmony ./dist/server.js",
    "state": "yarn build && node --harmony ./dist/state.js",
    "test": "jest"
  },
  "dependencies": {
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2",
    "koa-router": "^9.4.0",
    "telegraf": "^3.36.0",
    "uuid": "^8.3.0",
    "viber-bot": "^1.0.16",
    "xstate": "^4.9.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/koa": "^2.11.4",
    "@types/koa-bodyparser": "^4.3",
    "@types/koa-router": "^7.4.0",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.2",
    "shx": "^0.3.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9",
    "typescript": "^4.0.2"
  }
}
